{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport FormInput from \"@/components/FormInput.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    FormInput\n  },\n  data: () => ({\n    formInput: {\n      typeOfIncome: \"Income\",\n      comment: \"\",\n      value: 0\n    },\n    budgetList: [{\n      typeOfIncome: \"Income\",\n      comment: \"\",\n      value: 12\n    }],\n    totalValue: 0\n  }),\n  methods: {\n    addNewType() {\n      let newArray = {\n        typeOfIncome: this.formInput.typeOfIncome,\n        comment: this.formInput.comment,\n        value: this.formInput.value\n      };\n      this.budgetList.push(newArray);\n      this.formInput.typeOfIncome = \"\";\n      this.formInput.comment = \"\";\n      this.formInput.value = 0;\n    },\n    deleteItems(index) {\n      this.budgetList.splice(index, 1);\n    }\n  },\n  computed: {\n    totalValueAll() {\n      return this.budgetList.reduce((acc, item) => item.value + acc, 0);\n    }\n  }\n};","map":{"version":3,"mappings":";AAmCA,OAAOA,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IAAEF;EAAU,CAAC;EACzBG,IAAI,EAAE,OAAO;IACXC,SAAS,EAAE;MACTC,YAAY,EAAE,QAAQ;MACtBC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACT,CAAC;IACDC,UAAU,EAAE,CAAC;MAAEH,YAAY,EAAE,QAAQ;MAAEC,OAAO,EAAE,EAAE;MAAEC,KAAK,EAAE;IAAG,CAAC,CAAC;IAChEE,UAAU,EAAE;EACd,CAAC,CAAC;EAEFC,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,IAAIC,QAAO,GAAI;QACbP,YAAY,EAAE,IAAI,CAACD,SAAS,CAACC,YAAY;QACzCC,OAAO,EAAE,IAAI,CAACF,SAAS,CAACE,OAAO;QAC/BC,KAAK,EAAE,IAAI,CAACH,SAAS,CAACG;MACxB,CAAC;MACD,IAAI,CAACC,UAAU,CAACK,IAAI,CAACD,QAAQ,CAAC;MAC9B,IAAI,CAACR,SAAS,CAACC,YAAW,GAAI,EAAE;MAChC,IAAI,CAACD,SAAS,CAACE,OAAM,GAAI,EAAE;MAC3B,IAAI,CAACF,SAAS,CAACG,KAAI,GAAI,CAAC;IAC1B,CAAC;IACDO,WAAW,CAACC,KAAK,EAAE;MACjB,IAAI,CAACP,UAAU,CAACQ,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAClC;EACF,CAAC;EAEDE,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,OAAO,IAAI,CAACV,UAAU,CAACW,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKA,IAAI,CAACd,KAAI,GAAIa,GAAG,EAAE,CAAC,CAAC;IACnE;EACF;AA